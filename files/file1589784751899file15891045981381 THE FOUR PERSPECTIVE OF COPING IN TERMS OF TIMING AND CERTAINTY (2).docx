<!-- ****** Start The Markup content ****** -->
<div class="container">
<!-- ****** Start the search bar ****** -->
<div class="search-bar">
    <form>
        <input type="search" dir="auto" autocomplete="off" name="search-name" class="form-control" placeholder="Find someone">
        <button type="button" class="btn submit-filter btn-primary">
            <i class="fa fa-search"></i>
        </button>
    </form>
    <ul class="users-result list-unstyled" id="theusers"></ul>
</div>

<!-- ****** Add Btns and events ******-->

    <div class="notifications popup popup-notifications" data-popup="popup-notifications">
        <ul id='notificationsList' class="list-unstyled notificationsList popup-inner">
            
        </ul>
    </div>
    <div class="messages notifications popup popup-messages" data-popup="popup-messages">
        <ul class='popup-inner list-unstyled notificationsList'>
        
        </ul>
    </div>


<!-- ****** popups ****** -->
<div class="popup popup-1st" data-popup="popup-1st">
    <div class="popup-inner image">
        <form action="/profile/5ebacab9dde45b2f3c85f86c/post-statu" method="POST" enctype="multipart/form-data">
            <!-- *** User Info for posts *** -->
            <input type="hidden" name="mypicture" value="defaultUser.jpeg">
            <input type="hidden" name="myfirstname" value="Aissa">
            <input type="hidden" name="mylastname" value="Diafi">
            <input type="hidden" name='redirect' value='/5ebacab9dde45b2f3c85f86c'>
            <input type="file" name="image" accept="image/*" required>
            <input type="text" dir="auto" autocomplete="off" name="title" placeholder="Add Title" required class="form-control title">
            <textarea name="description" autocomplete="off" dir="auto" id="description" cols="15" rows="5" required required placeholder="Add Description" class="form-control article"></textarea>
            <h4>Category & Model:</h4>
            <input type="text" dir="auto" autocomplete="off" name="category" placeholder="Category" required class='form-control select'>
            <input type="text" dir="auto" autocomplete="off" name="model" placeholder="Model" required class='form-control select model'>
            <input type="submit" value="Post" class="btn btn-success submit" id="submit-post">
        </form>
        <button class='close-popup btn btn-danger'>Close</button>
    </div>
</div>
<div class="popup popup-2nd" data-popup="popup-2nd">
    <div class="popup-inner file">
        <form action="/profile/5ebacab9dde45b2f3c85f86c/post-file" method="POST" enctype="multipart/form-data">
            <!-- *** User Info for files *** -->
            <input type="hidden" name="mypicture" value="defaultUser.jpeg">
            <input type="hidden" name="myfirstname" value="Aissa">
            <input type="hidden" name="mylastname" value="Diafi">
            <input type="hidden" name='redirect' value='/5ebacab9dde45b2f3c85f86c'>
            <input type="file" name="file" required max="1" accept='.doc, .docx, .pdf, .xlsx, xls, .ppt, .pptx, .pub, .txt,application/vnd.ms-word, application/vnd.ms-excel, application/mspowerpoint, text/plain, application/pdf'>
            <input type="text" dir="auto" autocomplete="off" name="title" maxlength="25" required placeholder="Add Title" class="form-control title">
            <textarea name="description" autocomplete="off" dir="auto" cols="15" required rows="5" required placeholder="add Description" class="form-control article"></textarea>
            <h4>Category & Model:</h4>
            <input type="text" dir="auto" autocomplete="off" name="category" placeholder="Category" required class='form-control select'>
            <input type="text" dir="auto" autocomplete="off" name="model" placeholder="Model" required class='form-control select model'>
            <input type="submit" value="Post" class="btn btn-success submit">
        </form>
        <button class='close-popup btn btn-danger'>Close</button>
    </div>
</div>
</div>

